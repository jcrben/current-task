{
    "compilerOptions": {
        "noEmit": true,
        "allowJs": true,
        "checkJs": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "target": "ES2024"
    },
    "include": ["src/**/*"]
}
